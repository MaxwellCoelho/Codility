function solution(A) {
    let pairs = 0,
        P = 0;

    for (let n = 0; n < A.length; n++) {
        if (A[n] == 0) {
            P++;
        }
        
        if (A[n] == 1 && P > 0) {
            pairs += P;
        }

        if (pairs > 1000000000) {
            return -1;
        }
    }

    return pairs;
}
