function solution(A) {
    A.sort((a, b) => a - b);

    let result = [
        A[A.length - 1] * A[A.length - 2] * A[A.length - 3],
        A[0] * A[A.length - 1] * A[A.length - 2],
        A[1] * A[0] * A[A.length - 1],
        A[2] * A[1] * A[0],
    ];
    
    return Math.max.apply(null, result);
}
